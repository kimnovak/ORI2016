package view;

import java.awt.BorderLayout;

import javax.swing.BoxLayout;
import javax.swing.JCheckBox;
import javax.swing.JPanel;

public class PanelZemlje extends JPanel {

	public PanelZemlje() {
		setLayout(new BorderLayout());
		JPanel panel = new JPanel();
		panel.setLayout(new BoxLayout(this, BoxLayout.Y_AXIS));
		JCheckBox c0 = new JCheckBox("Albania");
		panel.add(c0);
        JCheckBox c1 = new JCheckBox("Andorra");
        panel.add(c1);
        JCheckBox c2 = new JCheckBox("Armenia");
        panel.add(c2);
        JCheckBox c3 = new JCheckBox("Austria");
        panel.add(c3);
        JCheckBox c4 = new JCheckBox("Azerbaijan");
        panel.add(c4);
        JCheckBox c5 = new JCheckBox("Belarus");
        panel.add(c5);
        this.add(panel, BorderLayout.WEST);
        
	}
}
